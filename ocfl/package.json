  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.33",
    "clean-jsdoc-theme": "^3.3.4",
    "jsdoc": "^3.6.10",
    "mocha": "^10.0.0"
  },
  "mocha": {
    "extension": [
      ".spec.js",
      ".spec.cjs",
      ".spec.mjs"
